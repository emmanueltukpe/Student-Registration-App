{
  "name": "football-game",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/server/index.js",
    "start:dev": "nodemon ./dist/server",
    "start:debug": "nodemon --inspect ./dist/server | bunyan -l debug",
    "watch:tsc": "tsc --watch -p ./tsconfig.json",
    "build:tsc": "rm -rf ./dist && tsc -p ./tsconfig.json",
    "compile": "tsc",
    "start:worker": "node ./dist/workers/index.js",
    "start:worker:dev": "nodemon ./dist/workers/index | bunyan",
    "start:worker:debug": "nodemon --inspect ./dist/server/workers/index | bunyan",
    "test": "rm -f logs.json && APP_ENV=test jest --runInBand --forceExit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "express-request-id": "^3.0.0",
    "helmet": "^7.0.0",
    "mongoose": "^7.3.1",
    "nanoid": "^4.0.2",
    "response-time": "^2.3.2",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-request-id": "^3.0.0",
    "@types/helmet": "^4.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/response-time": "^2.3.5",
    "nodemon": "^2.0.22"
  }
}
