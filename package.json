{
    "name": "football-game",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "node ./dist/server/index.js",
        "start:dev": "nodemon ./dist/server | bunyan -l debug",
        "start:debug": "nodemon --inspect ./dist/server | bunyan -l debug",
        "watch:tsc": "tsc --watch -p ./tsconfig.json",
        "build:tsc": "rm -rf ./dist && tsc -p ./tsconfig.json",
        "compile": "tsc",
        "start:worker": "node ./dist/workers/index.js",
        "start:worker:dev": "nodemon ./dist/workers/index | bunyan",
        "start:worker:debug": "nodemon --inspect ./dist/server/workers/index | bunyan",
        "test": "rm -f logs.json && APP_ENV=test jest --runInBand --forceExit"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "bunyan": "^1.8.12",
        "cors": "^2.8.5",
        "date-fns": "^2.18.0",
        "dotenv": "^6.2.0",
        "express": "^4.17.2",
        "express-request-id": "^1.4.1",
        "helmet": "^3.18.0",
        "http-status-codes": "^1.3.0",
        "joi": "^17.4.2",
        "jsonwebtoken": "^9.0.2",
        "mailgen": "^2.0.28",
        "module-alias": "^2.1.0",
        "mongoose": "^5.13.7",
        "multer": "^1.4.5-lts.1",
        "nanoid": "^3.1.25",
        "nock": "^12.0.3",
        "nodemailer": "^6.9.7",
        "prom-client": "^12.0.0",
        "response-time": "^2.3.2",
        "typescript": "^4.7.4",
        "validate-image-type": "^3.0.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/bunyan": "^1.8.5",
        "@types/cheerio": "^0.22.31",
        "@types/cors": "^2.8.12",
        "@types/date-fns": "^2.6.0",
        "@types/dotenv": "^6.1.0",
        "@types/express": "^4.16.0",
        "@types/express-request-id": "^1.4.3",
        "@types/faker": "^4.1.5",
        "@types/helmet": "^0.0.43",
        "@types/jest": "^26.0.23",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/morgan": "^1.7.35",
        "@types/multer": "^1.4.11",
        "@types/nodemailer": "^6.4.14",
        "@types/response-time": "^2.3.5",
        "@types/supertest": "^2.0.7",
        "faker": "^4.1.0",
        "jest": "^26.6.3",
        "nodemon": "^1.18.9",
        "supertest": "^3.4.2",
        "ts-jest": "^26.5.6",
        "ts-node": "^9.0.0"
    }
}
